
<ACCOUNTScreen>
    id: account
    name: 'account_screen'
    Account_box:
        id: account_box

<Account_box>
    id: account_box
    orientation: "vertical"

    BoxLayout:
        id: icon_box
        size_hint: .07,.1
        MDIconButton:
            icon: "windows/images/back_btn.png"
            on_press:root.back()
            #pos_hint: { 'center_y': .5}

    BoxLayout:
        id: title_box
        size_hint: .95,.1
        MDLabel:

            pos_hint_x: None
            pos_hint: {'center_x': 0.5}
            size_hint: .5,.5
            id : contact_us_label
            text : "My Account"
            font_size: 32

    ##############empty grid to make space between the "my account"label and the 3 horizontal buttons

    GridLayout:
        cols:1
        size_hint:.1,.1
    BoxLayout:
        id: choose_box
        orientation: 'horizontal'
        size_hint: 1,.1
        Button:
            id: personal_btn
            text:"Personal"
            pos_hint: {'top':1}
            #color: 0,0,0,0.65
            #size_hint_y: .1
            background_color: (0,0,0,0.11)
            color: (0,0,0,0.65) if self.state == 'normal' else (24/255,211/255,199/255,1)
            on_press: root.change_to_personal()
        Button:
            id: address_btn
            text:"Adderess"
            pos_hint: {'top':1}
            #color: 0,0,0,0.65
            #size_hint_y: .1
            background_color: (0,0,0,0.11)
            color: (0,0,0,0.65) if self.state == 'normal' else (24/255,211/255,199/255,1)
            on_press: root.change_to_address()
        Button:
            id: password_btn
            text:"Password"
            pos_hint: {'top':1}
            #color: 0,0,0,0.65
            #size_hint_y: .1
            background_color: (0,0,0,0.11)
            color: (0,0,0,0.65) if self.state == 'normal' else (24/255,211/255,199/255,1)
            on_press: root.change_to_update_password()

    BoxLayout:
        id: choose_box
        orientation: 'horizontal'
        size_hint: 1,.9
########################################################################################################################################################################

<Personal_box>:
    id:personal_box
    orientation: "vertical"
    size_hint: .5,.8
    pos_hint:{'center_y':.5}

    ScrollView:
        do_scroll_y:True

        GridLayout:
            cols:1

            size_hint_y: 3.0
            height: self.minimum_height
            pos_hint:{'center_y':.1}
            ############## grid of first name label and his text input

            GridLayout:
                cols:1
                size_hint_y: None
                height: self.minimum_height
                id: first_name_box
                #orientation: 'vertical'
                size_hint: .5,.6
                padding: ['20dp','30dp', '20dp' , '0dp']

                MDLabel:
                    id: first_name_label
                    text: "First Name"
                    font: 'sofia pro bold'
                    font_size:15

                TextInput:


                    id: first_name_input

                    size_hint_y: .5
                    write_tab: False
                    multiline: False

            ############## grid of last name label and his text input

            GridLayout:
                cols:1
                id: last_name_box
                #orientation: 'vertical'
                size_hint: .5,.6
                padding: ['20dp','30dp', '20dp' , '0dp']

                MDLabel:
                    id: last_name_label
                    text: "Last Name"
                    font: 'sofia pro bold'
                    font_size:15

                TextInput:
                    id:last_name_input
                    normal_color: 100,0,0,0
                    color_active: 100,100,1,0
                    size_hint_y: .5
                    write_tab: False
                    multiline: False

            ############## grid of email label and his text input

            GridLayout:
                cols:1
                id: email_box
                #orientation: 'vertical'
                size_hint: .5,.6
                padding: ['20dp','30dp', '20dp' , '0dp']
                MDLabel:
                    id: email_label
                    text: "Email"
                    font: 'sofia pro bold'
                    font_size:15

                TextInput:
                    id:email_input
                    normal_color: 100,0,0,0
                    color_active: 100,100,1,0
                    size_hint_y: .5
                    write_tab: False
                    multiline: False

            ############## grid of phone label and his text input

            GridLayout:
                cols:1
                id: phone_box
                #orientation: 'vertical'
                size_hint: .5,.6
                padding: ['20dp','30dp', '20dp' , '0dp']

                MDLabel:
                    id: phone_label
                    text: "Phone Number"
                    font: 'sofia pro bold'
                    font_size:14

                TextInput:
                    id:phone_input
                    normal_color: 100,0,0,0
                    color_active: 100,100,1,0
                    size_hint_y: .5
                    write_tab: False
                    multiline: False

            ############## grid of birth date label and his text input

            GridLayout:
                cols:1
                id: birth_date_box
                size_hint: .5,.6
                padding: ['20dp','30dp', '20dp' , '0dp']

                MDLabel:
                    id: birth_date_label
                    text: "Birth Date"
                    font: 'sofia pro bold'
                    font_size:20

                #grid layout of year, month, day

                GridLayout:
                    cols:3

                    pos_hint:{'center_x':.1, 'center_y':.5}
                    MDDropDownItem:
                        id:year_input
                        pos_hint: {'center_x': .1, 'center_y': .5}
                        text: 'Year'
                        font_name: "Arial"
                        normal_color: 100,0,0,0
                        color_active: 100,100,1,0
                        write_tab: False
                        multiline: False
                        on_release: root.show_dropdown_year()
                    MDDropDownItem:
                        id:month_input
                        pos_hint: {'center_x': .1, 'center_y': .5}
                        text: 'Month'
                        font_name: "Arial"
                        normal_color: 100,0,0,0
                        color_active: 100,100,1,0
                        write_tab: False
                        multiline: False
                        on_release: root.show_dropdown_month()
                    MDDropDownItem:
                        id:day_input
                        pos_hint: {'center_x': .1, 'center_y': .5}
                        text: 'Day'
                        font_name: "Arial"
                        normal_color: 100,0,0,0
                        color_active: 100,100,1,0
                        write_tab: False
                        multiline: False
                        on_release: root.show_dropdown_day()


            ############## grid of gender label and his text input

            GridLayout:
                cols:1
                size_hint: 0.6,.6
                #size:.5,.6


                padding: ['20dp','30dp', '20dp' , '0dp']
                id: Gender_box
                #orientation: 'horizontal'

                MDLabel:
                    id: gender_label
                    text: "Gender"
                    font: 'sofia pro bold'
                    font_size:20


                GridLayout:
                    cols:15
                    size_hint: .2,.5

                    CheckBox:
                        id: male
                        text: "Male"
                        group:1
                        on_active: root.save_gender(self, self.active, 'male')

                    MDLabel:
                        id: male_label
                        text: "Male"
                    #MDIconButton:
                        font_size:15
                     #   icon: "windows/images/male-user-icon.png"
                      #  id: male_photo


                    CheckBox:
                        id: female
                        text: 'Female'
                        group:1

                    MDLabel:
                        id: male_label
                        text: "Female"
                        font_size:15
                    #MDIconButton:
                     #   id: female_photo
                      #  icon: "windows/images/female-user-icon.png"

                    GridLayout:
                        cols:1
                    GridLayout:
                        cols:1
                    GridLayout:
                        cols:1
                    GridLayout:
                        cols:1
                    GridLayout:
                        cols:1
                    GridLayout:
                        cols:1
                    GridLayout:
                        cols:1
                    GridLayout:
                        cols:1
                    GridLayout:
                        cols:1
                    GridLayout:
                        cols:1
                    GridLayout:
                        cols:1



            ######################empty grid layout to make space

            GridLayout:
                cols:3
                size:.1,.1
                size_hint:.1,.1

            ############## grid of update button

            GridLayout:
                size:.5,.7
                size_hint: .5,.5
                pos_hint:{'center_x':.1, 'center_y':.1}
                cols:1
                id: update_box
                #padding: ['100dp','30dp', '100dp' , '10dp']
                Button:
                    size:0.2,0.2
                    id: login_btn
                    text: "Update"
                    font_size: 30
                    background_color: (0,0,0,0)
                    background_normal: ''
                    canvas.before:
                        Color:
                            rgba: (24/255,211/255,199/255,1)
                        RoundedRectangle:
                            size: self.size
                            pos: self.pos
                    on_press: root.personal()



