
<ACCOUNTScreen>
    id: account
    name: 'account_screen'
    Account_box:
        id: account_box

<Account_box>
    id: account_box
    orientation: "vertical"

    BoxLayout:
        id: icon_box
        size_hint: .07,.1
        MDIconButton:
            icon: "windows/images/back_btn.png"
            on_press:root.back()
            pos_hint: { 'center_y': .5}

    BoxLayout:
        id: title_box
        size_hint: .95,.1
        MDLabel:
            pos_hint: {'y': 0.4}
            #pos_hint: {'top':1, 'center':0.7}
            halign:'center'
            pos_hint_x: None
            size_hint: .5,.5
            id : contact_us_label
            text : "My Account"
            font_size: 32

    BoxLayout:
        id: choose_box
        orientation: 'horizontal'
        size_hint: 1,.1
        Button:
            id: personal_btn
            text:"Personal"
            pos_hint: {'top':1}
            #color: 0,0,0,0.65
            #size_hint_y: .1
            background_color: (0,0,0,0.11)
            color: (0,0,0,0.65) if self.state == 'normal' else (24/255,211/255,199/255,1)
            on_press: root.change_to_personal()
        Button:
            id: address_btn
            text:"Adderess"
            pos_hint: {'top':1}
            #color: 0,0,0,0.65
            #size_hint_y: .1
            background_color: (0,0,0,0.11)
            color: (0,0,0,0.65) if self.state == 'normal' else (24/255,211/255,199/255,1)
            on_press: root.change_to_address()
        Button:
            id: password_btn
            text:"Password"
            pos_hint: {'top':1}
            #color: 0,0,0,0.65
            #size_hint_y: .1
            background_color: (0,0,0,0.11)
            color: (0,0,0,0.65) if self.state == 'normal' else (24/255,211/255,199/255,1)
            on_press: root.change_to_update_password()

    BoxLayout:
        id: choose_box
        orientation: 'horizontal'
        size_hint: 1,.9


<Personal_box>:
    id:personal_box
    orientation: "vertical"
    size_hint: 1,1.2
    pos_hint:{'center_y':0.4}
    ScrollView:
        do_scroll_y:True
        GridLayout:
            cols:1
            size_hint_y: 3.5
            height: self.minimum_height
            GridLayout:
                cols:1
                size_hint_y: None
                height: self.minimum_height
                id: first_name_box
                #orientation: 'vertical'
                size_hint: 1,1.2
                padding: ['20dp','30dp', '20dp' , '0dp']

                MDLabel:
                    id: first_name_label
                    text: "First Name"
                    font: 'sofia pro bold'
                    font_size:15

                TextInput:
                    id: first_name_input
                    normal_color: 100,0,0,0
                    color_active: 100,100,1,0
                    size_hint_y: .5
                    write_tab: False
                    multiline: False


            GridLayout:
                cols:1
                id: last_name_box
                #orientation: 'vertical'
                size_hint: 1,1.2
                padding: ['20dp','30dp', '20dp' , '0dp']

                MDLabel:
                    id: last_name_label
                    text: "Last Name"
                    font: 'sofia pro bold'
                    font_size:15

                TextInput:
                    id:last_name_input
                    normal_color: 100,0,0,0
                    color_active: 100,100,1,0
                    size_hint_y: .5
                    write_tab: False
                    multiline: False

            GridLayout:
                cols:1
                id: email_box
                #orientation: 'vertical'
                size_hint: 1,1.2
                padding: ['20dp','30dp', '20dp' , '0dp']
                MDLabel:
                    id: email_label
                    text: "Email"
                    font: 'sofia pro bold'
                    font_size:15

                TextInput:
                    id:email_input
                    normal_color: 100,0,0,0
                    color_active: 100,100,1,0
                    size_hint_y: .5
                    write_tab: False
                    multiline: False

            GridLayout:
                cols:1
                id: phone_box
                #orientation: 'vertical'
                size_hint: 1,1.2
                padding: ['20dp','30dp', '20dp' , '0dp']

                MDLabel:
                    id: phone_label
                    text: "Phone Number"
                    font: 'sofia pro bold'
                    font_size:14

                TextInput:
                    id:phone_input
                    normal_color: 100,0,0,0
                    color_active: 100,100,1,0
                    size_hint_y: .5
                    write_tab: False
                    multiline: False

            GridLayout:
                cols:4
                id: birth_date_box
                #orientation: 'horizontal'
                size_hint: .6,.6
                padding: ['20dp','30dp', '20dp' , '0dp']

                MDLabel:
                    id: birth_date_label
                    text: "Birth Date"
                    font: 'sofia pro bold'
                    font_size:20

                MDDropDownItem:
                    id:year_input
                    pos_hint: {'center_x': .1, 'center_y': .5}
                    text: 'Year'
                    font_name: "Arial"
                    normal_color: 100,0,0,0
                    color_active: 100,100,1,0
                    write_tab: False
                    multiline: False
                    on_release: root.show_dropdown_year()
                MDDropDownItem:
                    id:month_input
                    pos_hint: {'center_x': .1, 'center_y': .5}
                    text: 'Month'
                    font_name: "Arial"
                    normal_color: 100,0,0,0
                    color_active: 100,100,1,0
                    write_tab: False
                    multiline: False
                    on_release: root.show_dropdown_month()
                MDDropDownItem:
                    id:day_input
                    pos_hint: {'center_x': .1, 'center_y': .5}
                    text: 'Day'
                    font_name: "Arial"
                    normal_color: 100,0,0,0
                    color_active: 100,100,1,0
                    write_tab: False
                    multiline: False
                    on_release: root.show_dropdown_day()
            GridLayout:
                cols:3

                padding: ['20dp','30dp', '20dp' , '0dp']
                id: Gender_box
                #orientation: 'horizontal'
                size_hint: 0.6,1.6
                MDLabel:
                    id: gender_label
                    text: "Gender"
                    font: 'sofia pro bold'
                    font_size:20



                GridLayout:
                    cols:1
                    id: gender_male_box
                    size: .5,.1
                    pos_hint:{'center_x':.3, 'center_y':0.5}
                    #orientation: 'vertical'


                    MDIconButton:
                        icon: "windows/images/male-user-icon.png"
                        id: male_photo
                        #text: "Male"
                        #font: 'sofia pro bold'
                        #font_size:10
                    MDIconButton:
                        id: female_photo
                        icon: "windows/images/female-user-icon.png"
                        #text: "Female"
                        #font: 'sofia pro bold'
                        #font_size:10

                GridLayout:
                    cols:1
                    id: gender_female_box
                    size: .5,0.9
                    pos_hint:{'center_x':0.9, 'center_y':0.5}
                    #orientation: 'vertical'
                    CheckBox:
                        id: male
                        text: "Male"
                        group:1
                        on_active: root.save_gender(self, self.active, 'male')
                    CheckBox:
                        id: female
                        text: 'Female'
                        group:1

                GridLayout:
                    #size:.5,.5
                    size_hint:.7,.5
                    pos_hint:{'center_x':.5, 'center_y':.5}
                    cols:1
                    id: update_box
                    #orientation: 'vertical'
                    #padding: ['100dp','30dp', '100dp' , '10dp']
                    Button:
                        size:0.2,0.2
                        id: login_btn
                        text: "Update"
                        font_size: 30
                        background_color: (0,0,0,0)
                        background_normal: ''
                        canvas.before:
                            Color:
                                rgba: (24/255,211/255,199/255,1)
                            RoundedRectangle:
                                size: self.size
                                pos: self.pos
                        on_press: root.personal()



