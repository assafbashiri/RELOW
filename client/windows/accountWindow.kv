
<ACCOUNTScreen>
    id: account
    name: 'account_screen'
    Account_box:
        id: account_box

<Account_box>
    id: account_box
    orientation: "vertical"

    BoxLayout:
        id: icon_box
        size_hint: .07,.1
        MDIconButton:
            icon: "windows/images/back_btn.png"
            on_press:root.back()
            pos_hint: { 'center_y': .5}

    BoxLayout:
        id: title_box
        size_hint: .95,.1
        MDLabel:
            pos_hint: {'y': 0.4}
            #pos_hint: {'top':1, 'center':0.7}
            halign:'center'
            pos_hint_x: None
            size_hint: .5,.5
            id : contact_us_label
            text : "My Account"
            font_size: 32

    BoxLayout:
        id: choose_box
        orientation: 'horizontal'
        size_hint: 1,.1
        Button:
            id: personal_btn
            text:"Personal"
            pos_hint: {'top':1}
            #color: 0,0,0,0.65
            #size_hint_y: .1
            background_color: (0,0,0,0.11)
            color: (0,0,0,0.65) if self.state == 'normal' else (24/255,211/255,199/255,1)
            on_press: root.change_to_personal()
        Button:
            id: address_btn
            text:"Adderess"
            pos_hint: {'top':1}
            #color: 0,0,0,0.65
            #size_hint_y: .1
            background_color: (0,0,0,0.11)
            color: (0,0,0,0.65) if self.state == 'normal' else (24/255,211/255,199/255,1)
            on_press: root.change_to_address()
        Button:
            id: password_btn
            text:"Password"
            pos_hint: {'top':1}
            #color: 0,0,0,0.65
            #size_hint_y: .1
            background_color: (0,0,0,0.11)
            color: (0,0,0,0.65) if self.state == 'normal' else (24/255,211/255,199/255,1)
            on_press: root.change_to_update_password()

    BoxLayout:
        id: choose_box
        orientation: 'horizontal'
        size_hint: 1,.9


<Personal_box>:
    id:personal_box
    orientation: "vertical"
    size_hint: 1,1.2
    pos_hint:{'center_y':0.9}

    BoxLayout:
        id: first_name_box
        orientation: 'vertical'
        size_hint: 1,1.2
        padding: ['20dp','30dp', '20dp' , '0dp']

        MDLabel:
            id: first_name_label
            text: "First Name"
            font: 'sofia pro bold'
            font_size:15

        TextInput:
            id: first_name_input
            normal_color: 100,0,0,0
            color_active: 100,100,1,0
            size_hint_y: 1.9
            write_tab: False
            multiline: False


    BoxLayout:
        id: last_name_box
        orientation: 'vertical'
        size_hint: 1,1.2
        padding: ['20dp','30dp', '20dp' , '0dp']

        MDLabel:
            id: last_name_label
            text: "Last Name"
            font: 'sofia pro bold'
            font_size:15

        TextInput:
            id:last_name_input
            normal_color: 100,0,0,0
            color_active: 100,100,1,0
            size_hint_y: 1.9
            write_tab: False
            multiline: False

    BoxLayout:
        id: email_box
        orientation: 'vertical'
        size_hint: 1,1.2
        padding: ['20dp','30dp', '20dp' , '0dp']
        MDLabel:
            id: email_label
            text: "Email"
            font: 'sofia pro bold'
            font_size:15

        TextInput:
            id:email_input
            normal_color: 100,0,0,0
            color_active: 100,100,1,0
            size_hint_y: 1.9
            write_tab: False
            multiline: False

    BoxLayout:
        id: phone_box
        orientation: 'vertical'
        size_hint: 1,1.2
        padding: ['20dp','30dp', '20dp' , '0dp']

        MDLabel:
            id: phone_label
            text: "Phone Number"
            font: 'sofia pro bold'
            font_size:14

        TextInput:
            id:phone_input
            normal_color: 100,0,0,0
            color_active: 100,100,1,0
            size_hint_y: 1.9
            write_tab: False
            multiline: False

    BoxLayout:
        id: birth_date_box
        orientation: 'vertical'
        size_hint: 1,1.2
        padding: ['20dp','30dp', '20dp' , '0dp']

        MDLabel:
            id: birth_date_label
            text: "Birth Date"
            font: 'sofia pro bold'
            font_size:15

        TextInput:
            id:birth_date_input
            normal_color: 100,0,0,0
            color_active: 100,100,1,0
            size_hint_y: 1.9
            write_tab: False
            multiline: False
            on_focus: root.show_date_picker()

    BoxLayout:
        id: male_female
        orientation: 'horizontal'
        size_hint: .2,.2
        canvas:
            Color:
                rgba: (.5,.5,.5,.5) # DarkOliveGreen
            Rectangle:
                size: self.size
                pos: self.pos
        pos_hint:{'center_x':.26, 'center_y':.5}
        padding: ['20dp','30dp', '20dp' , '0dp']
        MDLabel:
            id: gender_label
            text: "Male"
            font: 'sofia pro bold'
            font_size:10
        MDLabel:
            id: gender_label
            text: "Female"
            font: 'sofia pro bold'
            font_size:10
    BoxLayout:
        id: gender_cover
        orientation: 'vertical'
        size_hint: .2,.9
        padding: ['20dp','30dp', '20dp' , '0dp']
        MDLabel:
            id: gender_label
            text: "Gender"
            font: 'sofia pro bold'
            font_size:20
        BoxLayout:
            id: gender_box
            size: .1,.2
            pos_hint:{'center_x':1.4, 'center_y':1.7}
            orientation: 'horizontal'
            #padding: ['20dp','30dp', '20dp' , '0dp']

            CheckBox:
                id: male
                text: "Male"
                group:1
            CheckBox:
                id: female
                text: 'Female'
                group:1
    BoxLayout:
        id: update_box
        orientation: 'vertical'
        padding: ['100dp','30dp', '100dp' , '10dp']
        Button:
            id: login_btn
            text: "Update"
            font_size: 30
            background_color: (0,0,0,0)
            background_normal: ''
            canvas.before:
                Color:
                    rgba: (24/255,211/255,199/255,1)
                RoundedRectangle:
                    size: self.size
                    pos: self.pos
            on_press: root.personal()


