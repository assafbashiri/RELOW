#:import Label kivy.uix.button.Button

<RecycleViewRow>
    id: RecycleViewRow
    orientation: 'vertical'
    Carousel2:
        photos: self.parent.photo_lis
        id: car
    MDFloatingActionButton:
        icon: "windows/images/more3.png"
        on_press: root.www(self.parent.offer, self.parent.photo_lis)


<Offers_Screen>
    viewclass: 'RecycleViewRow'
    canvas.before:
        Color:
            rgb: 251/256, 255/256, 230/256
        Rectangle:
            pos: self.pos
            size: self.size
    RecycleBoxLayout:
        default_size: None, dp(500)
        default_size_hint: 1, None
        size_hint_y: None
        height: self.minimum_height
        orientation: 'vertical'
        spacing:10