<CONNECTScreen>
    id: connect
    name:'connect_screen'
    Connect_box:

<Connect_box>
    BoxLayout:
        orientation: 'vertical'
        canvas.before:
            Color:
                rgba: 0, 0, 0, 1
            Rectangle:
                pos: self.pos
                size: self.size
        Button:
            text: "REGISTER"
            on_press: root.register_new()
        Button:
            text: "LOGIN"
            on_press: root.login_new()



    Side_box:
        id:side_box


