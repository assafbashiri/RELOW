<ACCOUNTScreen>
    id: account
    name: 'account_screen'
    Account_box:
        id: account_box
    Down_menu:
        id: down_menu

<Account_box>
    id: account_box
    orientation: "vertical"

    BoxLayout:
        id: icon_box
        size_hint: 1,.1
        orientation: 'horizontal'
        spacing:250
        MDIconButton:
            icon: "assets/windows/images/back_btn.png"
            on_press:root.back()
        MDLabel:
            pos_hint_x: None
            pos_hint: {'center_x': 0.5}
            size_hint: .5,.5
            id : contact_us_label
            text : "My Account"
            font_size: 70

    ##############empty grid to make space between the "my account"label and the 3 horizontal buttons

    GridLayout:
        cols:1
        size_hint:.1,.1
    BoxLayout:
        id: choose_box
        orientation: 'horizontal'
        size_hint: 1,.1
        Button:
            id: personal_btn
            text:"Personal"
            pos_hint: {'top':1}
            #color: 0,0,0,0.65
            #size_hint_y: .1
            background_color: (0,0,0,0.11)
            color: (0,0,0,0.65) if self.state == 'normal' else (24/255,211/255,199/255,1)
            on_press: root.change_to_personal_screen()
        Button:
            id: address_btn
            text:"Adderess"
            pos_hint: {'top':1}
            #color: 0,0,0,0.65
            #size_hint_y: .1
            background_color: (0,0,0,0.11)
            color: (0,0,0,0.65) if self.state == 'normal' else (24/255,211/255,199/255,1)
            on_press: root.change_to_address_screen()
        Button:
            id: password_btn
            text:"Password"
            pos_hint: {'top':1}
            #color: 0,0,0,0.65
            #size_hint_y: .1
            background_color: (0,0,0,0.11)
            color: (0,0,0,0.65) if self.state == 'normal' else (24/255,211/255,199/255,1)
            on_press: root.change_to_update_password_screen()

    BoxLayout:
        id: choose_box
        orientation: 'horizontal'
        size_hint: 1,.9
########################################################################################################################################################################

<Personal_box>:
    id:personal_box
    orientation: "vertical"
    size_hint: 1,1
    ScrollView:
        do_scroll_y:True
        GridLayout:
            cols:1
            spacing: 35
            size_hint_y: 1.8
            #height: self.minimum_height
            pos_hint:{'center_y':.1}
            padding: [50,30,50,150]
            ############## grid of first name label and his text input
            GridLayout:
                id: first_name_box
                cols:1
                #size_hint_y: None
                #size_y: self.minimum_height
                size_hint: 1,.6

                MDLabel:
                    id: first_name_label
                    text: "First Name"
                    font: 'sofia pro bold'
                    font_size:40
                Txt_in:
                    id: first_name_input
                    size_hint_y: .5


            ############## grid of last name label and his text input

            GridLayout:
                cols:1
                id: last_name_box
                #orientation: 'vertical'
                size_hint: 1,.6

                MDLabel:
                    id: last_name_label
                    text: "Last Name"
                    font: 'sofia pro bold'
                    font_size:40

                Txt_in:
                    id:last_name_input
                    size_hint_y: .5


            ############## grid of email label and his text input

            GridLayout:
                cols:1
                id: email_box
                size_hint: 1,.6
                MDLabel:
                    id: email_label
                    text: "Email"
                    font: 'sofia pro bold'
                    font_size:40

                Txt_in:
                    id:email_input
                    size_hint_y: .5

            ############## grid of phone label and his text input

            GridLayout:
                cols:1
                size_hint: 1,.5
                id: phone_box
                #orientation: 'vertical'
                GridLayout:
                    cols:1
                    size_hint: 1,1
                    MDLabel:
                        id: phone_label
                        text: "Phone Number"
                        font_size:40

                    GridLayout:
                        cols:2
                        size_hint: 1,.1
                        MDDropDownItem:
                            id:area_input
                            #pos_hint: {'center_x': .1, 'center_y': .5}
                            text: '052'
                            font_name: "Arial"
                            normal_color: 100,0,0,0
                            color_active: 100,100,1,0
                            write_tab: False
                            multiline: False
                            on_release: root.show_dropdown_area()

                        Txt_in:
                            id:phone_input
                            max_text_length: 7
                            input_filter: 'int'
                            size_hint_y: .5


            ############## grid of birth date label and his text input

            GridLayout:
                cols:1
                padding: [0,20,0,0]
                id: birth_date_box
                size_hint: 1,1

                MDLabel:
                    id: birth_date_label
                    text: "Birth Date"
                    font_size:40

                #grid layout of year, month, day

                GridLayout:
                    cols:3
                    spacing: 10
                    pos_hint:{'center_x':.1, 'center_y':.5}
                    MDDropDownItem:
                        id:year_input
                        pos_hint: {'center_x': .1, 'center_y': .5}
                        text: 'Year'
                        font_name: "Arial"
                        normal_color: 100,0,0,0
                        color_active: 100,100,1,0
                        write_tab: False
                        multiline: False
                        on_release: root.show_dropdown_year()
                    MDDropDownItem:
                        id:month_input
                        pos_hint: {'center_x': .1, 'center_y': .5}
                        text: 'Month'
                        font_name: "Arial"
                        normal_color: 100,0,0,0
                        color_active: 100,100,1,0
                        write_tab: False
                        multiline: False
                        on_release: root.show_dropdown_month()
                    MDDropDownItem:
                        id:day_input
                        pos_hint: {'center_x': .1, 'center_y': .5}
                        text: 'Day'
                        font_name: "Arial"
                        normal_color: 100,0,0,0
                        color_active: 100,100,1,0
                        write_tab: False
                        multiline: False
                        on_release: root.show_dropdown_day()


            ############## grid of gender label and his text input

            GridLayout:
                cols:1
                size_hint: 1,.6
                id: Gender_box
                #orientation: 'horizontal'

                MDLabel:
                    id: gender_label
                    text: "Gender"
                    font: 'sofia pro bold'
                    font_size:40


                GridLayout:
                    cols:15
                    size_hint: 1,.5

                    MDCheckbox:
                        id: male
                        text: "Male"
                        group:1
                        #thumb_color:(24/255,211/255,199/255,1)
                        #selected_color:(24/255,211/255,199/255,1)
                        #disabled_color:(24/255,211/255,199/255,1)
                        on_active: root.save_male(self, self.active)

                    MDLabel:
                        id: male_label
                        text: "Male"
                        font_size:40

                    MDCheckbox:
                        id: female
                        text: "Female"
                        group:1
                        #thumb_color:(24/255,211/255,199/255,1)
                        #selected_color:(24/255,211/255,199/255,1)
                        #disabled_color:(24/255,211/255,199/255,1)
                        on_active: root.save_female(self, self.active)

                    MDLabel:
                        id: male_label
                        text: "Female"
                        font_size:40






            ############## grid of update button
            GridLayout:
                size_hint: .5,.7
                pos_hint:{'center_x':.1, 'center_y':.1}
                cols:3
                id: update_box
                padding: [0,0,0,0]
                Button:
                    size_hint :0.3,3
                    id: sign_up_btn
                    text: "Update"
                    font_size: 30
                    background_color: (0,0,0,0)
                    background_normal: ''
                    canvas.before:
                        Color:
                            rgba: (24/255,211/255,199/255,1)
                        RoundedRectangle:
                            size: self.size
                            pos: self.pos
                    on_press: root.update_personal_box()





<Address_box>:
    id:address_box
    orientation: 'vertical'
    size_hint: 1,1

    ScrollView:
        do_scroll_y:True
        GridLayout:
            cols:1
            size_hint_y:1.2
            padding: [50,30,50,150]
            height: self.minimum_height
            pos_hint:{'center_y':.1}
            GridLayout:
                cols:1
                id: city_box
                size_hint: 1,1
                MDLabel:
                    id: city_label
                    text: "City"
                    font: 'sofia pro bold'
                    font_size:40

                Txt_hebrew:
                    text:'reh'
                    id:city_input
                    hint_text:'ריע'
                    size_hint_y: .5
                    base_direction:'rtl'
                    on_text:root.drop_cities_autocomplete(self.text)

            GridLayout:
                cols:1
                id: street_box
                #orientation: 'vertical'
                size_hint: 1,1

                MDLabel:
                    id: street_label
                    text: "Street"
                    font: 'sofia pro bold'
                    font_size:40

                Txt_hebrew:
                    id: street_input
                    hint_text:'בוחר'
                    size_hint_y: .5
                    base_direction:'rtl'
                    on_text:root.drop_streets_autocomplete(self.text)

            GridLayout:
                cols:1
                id: building_box
                #orientation: 'vertical'
                size_hint: 1,1

                MDLabel:
                    id: building_label
                    text: "Building"
                    font: 'sofia pro bold'
                    font_size:40
                Txt_in:
                    id: building_input
                    size_hint_y: .5
                    base_direction:'rtl'
                    text_language:'None'

            GridLayout:
                cols:1
                id: apt_box
                #orientation: 'vertical'
                size_hint: 1,1

                MDLabel:
                    id: apt_label
                    text: "Apartment No."
                    font: 'sofia pro bold'
                    font_size:40
                GridLayout:
                    cols:1
                    size_hint_y:.5
                Txt_in:
                    id: apt_input
                    size_hint_y: .5
                    input_filter: 'int'

            GridLayout:
                cols:1
                id: zip_code_box
                #orientation: 'vertical'
                size_hint: 1,1

                MDLabel:
                    id: zip_code_label
                    text: "Zip-Code"
                    font: 'sofia pro bold'
                    font_size:40
                GridLayout:
                    cols:1
                    size_hint_y:.2
                Txt_in:
                    id: zip_code_input
                    size_hint_y: .5
                    input_filter: 'int'
            GridLayout:
                cols:1
                id: update_box
                #orientation: 'vertical'
                padding: [0,30,0,0]
                Button:
                    id: login_btn
                    text: "Update"
                    font_size: 30
                    background_color: (0,0,0,0)
                    background_normal: ''
                    canvas.before:
                        Color:
                            rgba: (24/255,211/255,199/255,1)
                        RoundedRectangle:
                            size: self.size
                            pos: self.pos
                    on_press: root.update_address()


